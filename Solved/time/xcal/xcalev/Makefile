DEFINES=-DVER_VEC
# BINDIR=/usr/local/bin
INSTPGMFLAGS=$(INSTBINFLAGS)

all:: xcalev

           OBJS = xcalev.o
           SRCS = xcalev.c

        PROGRAM = xcalev

all:: xcalev

xcalev: $(OBJS) $(DEPLIBS)
	$(RM) $@
	$(CCLINK) -o $@ $(LDOPTIONS) $(OBJS) $(LOCAL_LIBRARIES) $(LDLIBS)  $(EXTRA_LOAD_FLAGS)

install:: xcalev
	@if [ -d $(DESTDIR)$(BINDIR) ]; then \
		set +x; \
	else \
		if [ -h $(DESTDIR)$(BINDIR) ]; then \
			(set -x; rm -f $(DESTDIR)$(BINDIR)) || exit $$?; \
		fi; \
		(set -x; $(MKDIRHIER) $(DESTDIR)$(BINDIR)) || exit $$?; \
	fi
	$(INSTALL) $(INSTALLFLAGS) $(INSTPGMFLAGS)  xcalev $(DESTDIR)$(BINDIR)/xcalev

all:: xcalev.$(MANNEWSUFFIX)

xcalev.$(MANNEWSUFFIX): xcalev.$(MANSRCSUFFIX)
	$(RM) $@
	if test -z "$(PREPROCESSMANPAGES)" ; then \
	   cd `dirname xcalev` && \
	   $(LN) `basename xcalev.$(MANSRCSUFFIX)` `basename $@`; \
	else \
	      	     $(RAWCPP)  $(MANDEFS) $(EXTRAMANDEFS) \
	     < xcalev.$(MANSRCSUFFIX) | sed -e '/^#  *[0-9][0-9]*  *.*$$/d' 			-e '/^#line  *[0-9][0-9]*  *.*$$/d' 			-e '/^[ 	]*XCOMM$$/s/XCOMM/#/' 			-e '/^[ 	]*XCOMM[^a-zA-Z0-9_]/s/XCOMM/#/' 			-e '/^[ 	]*XHASH/s/XHASH/#/' 			-e '/\@\@$$/s/\@\@$$/\\/' >$@; \
	fi

cleandir::
	$(RM) xcalev.$(MANNEWSUFFIX)

install.man:: xcalev.$(MANNEWSUFFIX)
	@if [ -d $(DESTDIR)$(MANDIR) ]; then \
		set +x; \
	else \
		if [ -h $(DESTDIR)$(MANDIR) ]; then \
			(set -x; rm -f $(DESTDIR)$(MANDIR)) || exit $$?; \
		fi; \
		(set -x; $(MKDIRHIER) $(DESTDIR)$(MANDIR)) || exit $$?; \
	fi
	$(INSTALL) $(INSTALLFLAGS) $(INSTMANFLAGS) xcalev.$(MANNEWSUFFIX) $(DESTDIR)$(MANDIR)/xcalev.$(MANSUFFIX)

depend::
	$(DEPEND) $(DEPENDFLAGS) -- $(ALLDEFINES) $(DEPEND_DEFINES) -- $(SRCS)

lint:
	$(LINT) $(LINTFLAGS) $(SRCS) $(LINTLIBS)
lint1:
	$(LINT) $(LINTFLAGS) $(FILE) $(LINTLIBS)

cleandir::
	$(RM) xcalev

